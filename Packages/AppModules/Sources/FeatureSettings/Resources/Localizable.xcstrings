    "settings.difficulty.hint_primary": {
      "extractionState": "manual",
      "localizations": {
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Larger grids are more difficult. In the widget, letters and touch targets are reduced so the board fits."
          }
        },
        "es": {
          "stringUnit": {
            "state": "translated",
            "value": "A mayor tamaño, más dificultad. En el widget las letras y el área táctil se reducen para que entre la cuadrícula."
          }
        }
      }
    },
